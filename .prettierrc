{
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "bracketSameLine": true,
  "importOrder": [
    "^node:(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^[./]"
  ],
  "importOrderSortSpecifiers": true,
  "importOrderSideEffects": false,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
