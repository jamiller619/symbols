{
  "name": "symbols",
  "version": "0.0.3",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/components/*.d.ts",
      "import": "./dist/components/*.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "node src/install.js && node src/browser.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "execa": "^9.6.1",
    "prettier": "^3.7.4",
    "sf-symbols-svg": "^7.0.1"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1"
  }
}
